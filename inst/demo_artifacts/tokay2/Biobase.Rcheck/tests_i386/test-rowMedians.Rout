> # rowMedians() by rowQ()
> rowMedians2 <- function(imat) {
+   nr <- ncol(imat)
+   half <- (nr + 1)/2
+   if (nr%%2 == 1) {
+     return(rowQ(imat, half))
+   } else {
+     return((rowQ(imat, half) + rowQ(imat, half+1))/2)
+   }
+ }
> 
> cat("Consistency checks:\n")
